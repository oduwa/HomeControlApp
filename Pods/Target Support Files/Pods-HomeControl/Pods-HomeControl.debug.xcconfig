EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/ChromaColorPicker" "$PODS_CONFIGURATION_BUILD_DIR/MZAppearance" "$PODS_CONFIGURATION_BUILD_DIR/MZFormSheetPresentationController" "$PODS_CONFIGURATION_BUILD_DIR/StepSlider"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/ChromaColorPicker/ChromaColorPicker.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MZAppearance/MZAppearance.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MZFormSheetPresentationController/MZFormSheetPresentationController.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/StepSlider/StepSlider.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "ChromaColorPicker" -framework "MZAppearance" -framework "MZFormSheetPresentationController" -framework "StepSlider"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
